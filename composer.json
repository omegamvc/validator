{
    "name": "omegamvc/validator",
    "version": "2.0.0",
    "description": "Php Validator",
    "keywords": ["php", "validator", "package"],
    "homepage": "https://omegamvc.github.io",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Adriano Giovannini",
            "email": "agisoftt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-intl": "*",
        "wixel/gump": "^v2.2.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Omega\\Validator\\": "src/Omega/Validator/"
        },
        "files": [
            "src/Omega/Validator/helper.php"
        ]
    },
    "autoload-dev": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
        }
    },
    "scripts": {
    }
}
